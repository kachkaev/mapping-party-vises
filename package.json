{
  "private": true,
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "next build && next export",
    "dev": "next dev",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:eslint": "eslint --fix \"**/*\"",
    "fix:markdownlint": "markdownlint --fix \"**/*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-deduplicate": "yarn install && yarn-deduplicate --strategy=fewer && yarn install",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:eslint": "eslint \"**/*\" --report-unused-disable-directives",
    "lint:markdownlint": "markdownlint \"**/*\"",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "prepare": "husky install",
    "start": "next start"
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "markdownlint --fix",
      "prettier --write"
    ]
  },
  "resolutions": {
    "@mapbox/geojson-rewind": "^0.5.0",
    "xmldom": "^0.5.0"
  },
  "dependencies": {
    "@turf/turf": "^6.3.0",
    "@visx/geo": "^1.7.0",
    "date-fns": "^2.19.0",
    "execa": "^5.0.0",
    "fs-extra": "^9.1.0",
    "lodash": "^4.17.21",
    "next": "^10.1.3",
    "osmtogeojson": "^3.0.0-beta.4",
    "react": "^17.0.2",
    "react-compare-slider": "^2.0.2",
    "react-dom": "^17.0.2",
    "react-use": "^17.2.1",
    "styled-components": "^5.2.3",
    "styled-normalize": "^8.0.7",
    "xmldom": "^0.5.0"
  },
  "devDependencies": {
    "@hkdobrev/run-if-changed": "^0.3.1",
    "@kachkaev/eslint-config-react": "^0.2.3",
    "@kachkaev/markdownlint-config": "^0.2.0",
    "@kachkaev/prettier-config": "^1.2.0",
    "@types/fs-extra": "^9.0.10",
    "@types/lodash": "^4.14.168",
    "@types/styled-components": "^5.1.9",
    "@types/xmldom": "^0.1.30",
    "eslint": "^7.23.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "markdownlint-cli": "^0.27.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3",
    "yarn-deduplicate": "^3.1.0"
  },
  "run-if-changed": {
    "yarn.lock": "yarn install --prefer-offline --pure-lockfile"
  }
}
